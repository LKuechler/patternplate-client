{
  "name": "patternplate-client",
  "version": "0.2.1",
  "description": "Client for patternplate-server",
  "main": "library/",
  "scripts": {
    "start": "node --harmony binary/patternplate-client.js",
    "build": "npm run babel",
    "watch": "npm run babel -- --watch",
    "babel": "npm run clean && babel source --out-dir ./",
    "clean": "rm -rf application configuration binary library"
  },
  "bin": {
    "patternplate-client": "binary/patternplate-client.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.sinnerschrader.com:patternplate/patternplate-client.git"
  },
  "keywords": [
    "patternplate"
  ],
  "author": {
    "name": "Team UX",
    "email": "team-ux@sinnerschrader.com"
  },
  "contributors": [
    {
      "name": "Mario Nebl",
      "email": "mario.nebl@sinnerschrader.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "iojs": ">= 1.4.3",
    "node": ">= 0.12.0",
    "npm": ">= 2.5.1"
  },
  "dependencies": {
    "babel-core": "^5.1.13",
    "babel-runtime": "^5.2.9",
    "babelify": "^6.0.2",
    "boilerplate-server": "git+ssh://git@git.sinnerschrader.com:patternplate/boilerplate-server.git#v0.4.1",
    "browserify": "^10.0.0",
    "camel-case": "^1.1.1",
    "classnames": "^1.2.2",
    "es6-promise": "^2.1.1",
    "highlight.js": "^8.5.0",
