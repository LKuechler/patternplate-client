    "less-plugin-npm-import": "^2.0.0",
    "markdown-css-themes": "git+ssh://git@git.sinnerschrader.com:frontend/markdown-css.git",
    "merge": "^1.2.0",
    "minimist": "^1.1.1",
    "moment": "^2.10.2",
    "normalize-opentype.css": "^0.2.4",
    "normalize.less": "^1.0.0",
    "pretty-data": "^0.40.0",
    "q-io": "^1.12.0",
    "rc": "^1.0.1",
